<!DOCTYPE html>
<html>
  <head>
    <title>MembersOnly</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <nav>
    <h1> Blogger </h1>
    <% if user_signed_in? %>
      <p> <%= link_to  current_user.name, edit_user_registration_path %> </p>
      <p> <%= link_to "Log Out", destroy_user_session_path,{method: 'delete'} %></p>
    <% else %>
      <p> <%= link_to  "Sign in", new_user_session_path %> </p>
      <p> <%= link_to  "Sign up", new_user_registration_path %> </p>
    <% end %>
  </nav>
    <%= yield %>
  </body>
</html>
